{
	"compilerOptions": {
		"target": "ES6",
		"lib": ["dom", "es2017"],
		"jsx": "react",
		"strict": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": ["src/**/*.tsx", "src/**/*.ts"]
}
